//    find the palandrom 

#include<stdio.h>
void main(){
    
    int n,s,r;
     s= 0;
     
     n = printf("enter the number: ");
    scanf("%d",&n);
    
      int t = n;
    while(n!= 0 ){
        r =n%10;
        s= s*10+r;
        n = n/10;
    }
    printf(" fibonake series is : %d\n",s);
    if(t == s){
        printf("number is palandrom");
    }
    else{
        printf("number is not palandrom");
    }
    
}
